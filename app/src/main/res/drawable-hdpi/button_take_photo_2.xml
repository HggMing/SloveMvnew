<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item
        android:bottom="80dp"
        android:left="80dp"
        android:right="80dp"
        android:top="80dp">
        <shape android:shape="oval">
            <solid android:color="@color/theme_color_primary" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="13dp"
                android:color="@color/font_black_2"
                android:dashWidth="0dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="7dp"
                android:color="@color/theme_color_primary"
                android:dashWidth="0dp" />
        </shape>
    </item>
</layer-list>